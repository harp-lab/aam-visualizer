{
  "name": "aam-visualizer",
  "version": "0.4.0",
  "description": "",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/harp-lab/aam-visualizer"
  },
  "main": "index.js",
  "config": {
    "frameworkDir": "framework",
    "fextDir": "fext",
    "fextConfig": "fext.config.js"
  },
  "nodemonConfig": {
    "watch": ["framework/server"]
  },
  "scripts": {
    "build": "webpack --config $npm_package_config_frameworkDir/webpack.config.js",
    "release": "npm version patch --no-git-tag-version && npm run build",
    "start": "node $npm_package_config_frameworkDir/server/main.js",
    "dev": "npm run dev-client & nodemon -x \"npm run dev-server\"",
    "dev-client": "webpack-dev-server --config $npm_package_config_frameworkDir/webpack.config.js",
    "dev-server": "NODE_ENV=development npm run start",
    "docs": "jsdoc -c $npm_package_config_frameworkDir/jsdoc.config.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^4.9.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.9.0",
    "codemirror": "^5.51.0",
    "cytoscape": "^3.14.0",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jsdoc": "^3.6.3",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "redux-devtools-extension": "^2.13.8",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
}
